<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elcatrin.app_delivery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.CartActivity"></activity>
        <activity
            android:name=".activity.StoresActivity"
            android:parentActivityName=".activity.ProductsActivity" />
        <activity
            android:name=".activity.ProductsActivity"
            android:parentActivityName=".activity.ProductDetailActivity" />
        <activity android:name=".activity.ProductDetailActivity" /> <!-- android:parentActivityName=".ui.CatalogoEmpresas" /> -->
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>